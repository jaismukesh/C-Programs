#include <iostream>
#include<limits.h>
using namespace std;
int main() {
  int a[10];
int maxi=INT_MIN;
  int n;
  cout<<"size of array"<<endl;
  cin>>n;

  cout<<"enter the element of array"<<endl;
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  for(int i=0;i<n;i++){
    if(maxi<a[i]){
      maxi=a[i];
    }
  }
  cout<<"Maximum number is:"<<maxi<<endl;
  return 0;
  }
