 #include <iostream>
#include<vector>
using namespace std;

int main()
{
    int arr[5]={1,2,3,4,5};
   int sizeA=5;
    int brr[5]={6,7,8,9,10};
   int sizeB=5;
    vector<int>ans;
    
    //push back element into ans array of arr//
    
     for(int i=0;i<sizeA;i++){
        ans.push_back(arr[i]);
    }
    //push back element into ans array of brr//
     for(int i=0;i<sizeB;i++){
        ans.push_back(brr[i]);
    }
    
    for(int i=0;i<ans.size();i++){
        cout<<ans[i]<<" ";
    }
    
    return 0;
}
