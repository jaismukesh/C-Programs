#include <iostream>
#include<vector>
#include<limits.h>
using namespace std;

int main()
{
   vector<int>arr{1,2,3,3,4,5};
    vector<int> ans;
   int sum=6;
   for(int i=0;i<arr.size();i++){
       int Element1=arr[i];
       
       for(int j=i+1;j<arr.size();j++){
           
           if(Element1+arr[j]==sum){
               
              cout<<"pair found: "<<Element1<<","<<arr[j]<<endl;
           }
       }
       
   }
   
 
    return 0;
}
