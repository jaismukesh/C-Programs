#include <iostream>
#include<limits.h>
using namespace std;
int main() {
  int a[10];
int min=INT_MAX;
  int n;
  cout<<"size of array"<<endl;
  cin>>n;

  cout<<"enter the element of array"<<endl;
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  for(int i=0;i<n;i++){
    if(min>a[i]){
      min=a[i];
    }
  }
  cout<<"Minimum number is:"<<min<<endl;
  return 0;
  }
