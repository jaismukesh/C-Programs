

#include <iostream>

using namespace std;
bool linearSearch(int a[],int n,int key){
  for(int i=0;i<5;i++){
    if(a[i]==key){
      return true;
      }
    }
}

int main() {
  int a[100];
int n;
cout<<"enter the size of array"<<endl;
  cin>>n;
int key;
  cout<<"enter the key value"<<endl;
  cin>>key;
  cout<<"enter the element of array"<<endl;
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  
  if(linearSearch(a,n,key)){
      cout<<"Found"<<endl;
  }
  else{
      cout<<"Not Found"<<endl;
  }
  return 0;
}
