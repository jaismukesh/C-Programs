#include <iostream>
#include<limits.h>
using namespace std;
int main() {
  int a[10];
  int n;
  cout<<"size of array"<<endl;
  cin>>n;
  int start=0;
  int end=n-1;

  cout<<"enter the element of array"<<endl;
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  while(true){
    if(start>end){
      break;
    }
    if(start==end){
      cout<<a[start]<<" ";
    }
    else{
      cout<<a[start]<<" ";
      cout<<a[end]<<" ";
    }
    end--;
    start++;
  }
  
  return 0;
  }
