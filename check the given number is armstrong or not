#include <iostream>

using namespace std;

int main()
{
    int n;
    cout<<"enter the value:"<<endl;
    cin>>n;
    int m=n;
    
    int sum=0;
    
    while(n>0){
        int r=n%10;
        n=n/10;
        sum=sum+r*r*r;
    }
    
    if(sum==m){
        cout<<"number is Armstrong Number"<<endl;
    }
    else{
        cout<<"number is not :";
    }

    return 0;
}
